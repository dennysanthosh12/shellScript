keytool -import -alias maven_central -keystore "C:\Path\To\Your\JDK\lib\security\cacerts" -file "C:\path\to\maven.cer"

mvn dependency:go-offline -Dmaven.wagon.http.ssl.insecure=true -Dmaven.wagon.http.ssl.allowall=true -Dmaven.wagon.http.ssl.ignore.validity.dates=true

<mirrors>
  <mirror>
    <id>central-http</id>
    <name>Central over HTTP</name>
    <url>http://repo.maven.apache.org/maven2</url>
    <mirrorOf>central</mirrorOf>
  </mirror>
</mirrors>

