@Entity
@Table(
  uniqueConstraints =
    @UniqueConstraint(columnNames = {"host","port"})
)
@Data
@NoArgsConstructor
public class NodeEndpoint {

    @Id
    @GeneratedValue(strategy = GenerationType.IDENTITY)
    private Long id;

    private String host;
    private int port;
    private String protocol;
    private boolean active;
}
